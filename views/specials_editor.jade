div(class="modal-dialog")
  div(class="modal-content")
    div(class="modal-header")
      button(type="button" class="close" data-dismiss="modal")
        span(aria-hidden="true") &times;
        span(class="sr-only") Close
      h4(class="modal-title" id="myModalLabel") Specials
    div(class="modal-body")
      form(class="form-horizontal" role="form")
        div( class="form-group")
          table(class='table')
            each row in value
              tr
                td
                  =row.CiscoKey
                td
                  =row.Value
                td 
                  a(class="btn btn-danger",onClick="del('Specials',"+row.ID+");return false;")
                    | Del
            tr
              td
                input(class="form-control",type="text", id='new_CiscoKey', placeholder='CiscoKey')
              td
                input(class="form-control",type="text", id='new_Value', placeholder='Value')
              td
                input(type="hidden", id='new_Phone_ID', value=id)
                button(class="btn btn-primary",onClick="add('Specials');return false;")
                  | Add
                
                
    div(class="modal-footer")
      button(type="button" class="btn btn-default" data-dismiss="modal") Close
- var show = "$('#"+element+"').modal('show');"
script
  =show
